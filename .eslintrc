{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "google"
  ],
  "rules": {
    "indent": ["error", 4],
    "linebreak-style": "off",
    "max-len": ["error", 120],
    "no-useless-constructor": 0
  }
}